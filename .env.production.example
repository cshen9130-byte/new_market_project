# Client-side MOM report alias (served by Nginx)
NEXT_PUBLIC_MOM_REPORT_URL=/mom_report/report.html

# Credentials for EmQuant API (set on server, do not commit real values)
EMQ_USERNAME=
EMQ_PASSWORD=
EMQ_OPTIONS_EXTRA=LoginType=2

# Python executable for server venv
PYTHON_EXE=/root/new_market_project/.venv/bin/python3

# Tushare token for Python data fetchers
TUSHARE_TOKEN=

# Optional library path if needed by EmQuant
LD_LIBRARY_PATH=/root/new_market_project/EMQuantAPI_Python/EMQuantAPI_Python/python3/libs/linux/x64